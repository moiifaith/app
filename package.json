{
  "name": "vue-app",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@10.9.2",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev:local": "npm run build && wrangler pages dev dist --local --port 8080",
    "dev:remote": "npm run build && wrangler pages dev dist --port 8080",
    "db:execute": "wrangler d1 execute zikr-database --local",
    "db:execute-remote": "wrangler d1 execute zikr-database"
  },
  "dependencies": {
    "@capacitor-community/safe-area": "^7.0.0-alpha.1",
    "@capacitor/android": "^7.4.3",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/ios": "^7.4.3",
    "@capacitor/preferences": "^7.0.2",
    "@ionic/vue": "^8.7.4",
    "@ionic/vue-router": "^8.7.4",
    "bcryptjs": "^3.0.2",
    "core-js": "^3.8.3",
    "jsonwebtoken": "^9.0.2",
    "vue": "^3.2.13",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
